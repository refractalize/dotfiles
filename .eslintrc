{
  "plugins": [
    "react",
    "babel"
  ],
  "settings": {
    "react": {
      "pragma": "plastiq"
    }
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-unused-vars": 1,
    "no-console": 0,
    "comma-dangle": [2, "only-multiline"]
  },
  "globals": {
    "serverRequire": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  }
}
